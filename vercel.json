{
    "version": 2,
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python",
        "config": {
          "build": {
            "command": "python -m pip install -r requirements.txt"
          }
        }
      }
    ]
  }
  